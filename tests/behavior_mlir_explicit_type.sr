package behavior_mlir_explicit_type

testing :: import "std/testing"

test "behavior: mlir op with explicit void result" {
    mlir op : void {} 
}

test "behavior: mlir op with explicit i32 result" {
    x := mlir op : i32 { "arith.constant"() {value = 42 : i32} : () -> i32 }
    testing.expect(x == 42, "mlir op explicit i32")!
}