package main

printf :: extern proc(string, any) i32

map :: import "std/map"

MapStrI32 :: map.Map(string, i32)

main :: proc() i32 {
  m := MapStrI32.init()
  m.set("one", 1)
  m.set("two", 2)

  value := m.get("one")
  if value != null {
    printf("one=%d\n", value?)
  } else {
    printf("missing\n", 0)
  }
  _ = m.remove("one")
  printf("len=%d\n", m.len)
  return 0
}
